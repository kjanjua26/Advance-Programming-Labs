<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="emp1" class="lab10.labSpring.Employee">
		<property name="empName" value="Kamran"></property>
		<property name="empID" value="2"></property>
		<property name="empDeps">
		<list>
			<value>Managerial</value>
			<value>Development</value>
			<value>Quality Testing</value>
			<value>Designing</value>
		</list>
		</property>
	</bean>
</beans>